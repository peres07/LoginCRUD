{
    "compilerOptions": {
      
        "target": "ES6",
        "module": "ES6",
        "moduleResolution": "Node",
        "allowJs": true,
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "useUnknownInCatchVariables": false,
        "strict": true,
        "skipLibCheck": true,
    }
}
